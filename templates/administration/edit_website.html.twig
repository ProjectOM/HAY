{{ form_start(edit_website) }}
<div class="tabContent" id="edit_website">
    <h1>{{ 'edit_website'|trans }}</h1>
    <label><img src="/ressources/HAYlogo.svg" width="256px" height="256px" /><br />{{ 'image_type_svg'|trans }} {{ 'logo'|trans }}<br />{{ form_widget(edit_website.HAYlogo, {'attr': {'accept': 'image/svg+xml'}}) }}</label>
    <label><img src="/ressources/icon.svg" width="256px" height="256px" /><br />{{ 'image_type_svg'|trans }} {{ 'default_profile_picture'|trans }}<br />{{ form_widget(edit_website.icon, {'attr': {'accept': 'image/svg+xml'}}) }}</label>
    <label class="submit" style="grid-column: 1/3;" for="form_version">{{ 'show_version'|trans }}{{ form_widget(edit_website.version) }}</label>
    {{ form_widget(edit_website.submit, {'attr': {'class': 'submit', 'style': 'grid-column: 1/3;'}, 'label': 'edit'|trans}) }}
</div>
{{ form_end(edit_website) }}
